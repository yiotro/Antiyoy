    <string key="help_units_article" value="Jsou čtyři typy jednotek
        #Liší se pouze silou.
        #Vzestupně podle síly:
        #[1] rolník
        #[2] kopitník
        #[3] baron
        #[4] rytíř
        #
        #Jednotky dávají okolním
        #hexům ochranu rovnou jejich
        #síle.
        #
        #Pro obsazení hexu musí být
        #síla útočné jednotky větší
        #než obrana hexu.
        #
        #Každá jednotka spotřebuje
        #2, 6, 18 respektive 54
        #mincí za jeden tah.
        "/>

    <string key="help_trees_article" value="Stromy vyrostou tam,
        #kde zemře jednotka.
        #
        #Hexy se stromy nedávají žádný
        #příjem. Stromy tedy poškozují
        #ekonomiku.
        #
        #Musíš je vykácet, jinak se
        #rozrostou a vyhladoví tě.
        #
        #Palmy se objevují v rozích
        #a rostou velmi rychle.
        #Musí být tedy i velmi rychle
        #vykáceny.
        "/>

    <string key="help_towers_article" value="Věže ochraňují okolní hexy.
        #
        #Veže dávají ochranu [2].
        #Tudíš jen baroni a rytíři
        #je mohou porazit.
        #
        #Domky dávají ochranu [1].
        #
        #Na počátku hry je toto
        #nejlepší způsob, jak
        #ochránit své teritorium.
        #Cena věží je nízká
        #a nespotřebovávají žádné
        #peníze.
        "/>

    <string key="help_money_article" value="Příjem závisí na počtu
        #hexů. Větší území má větší
        #příjem.
        #
        #Stromy příjem omezují.
        #
        #Jednotky spotřebovávají
        #peníze. Silnější jednotky
        #jich potřebují víc.
        #
        #Když dojdou peníze,
        #jednotky umřou.
        #
        #Příjem se ukazuje
        #nahoře uprostřed.
        "/>

    <string key="help_tactics_article" value="Na počátku hry obsaď
        #neutrální hexy.
        #Poté ochraňuj své území.
        #
        #Ujisti se, že tvůj příjem
        #je dostatečně velký,
        #než postavíš nové jednotky.
        #
        #Rozkouskuj území nepřítele
        #pro zničení jeho ekonomiky.
        "/>

    <string key="about_complicated_mode_article" value="Pokročilý mód znamená
