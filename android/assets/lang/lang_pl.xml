    <string key="help_units_article" value="Są 4 rodzaje jednostek.
        #Różnią się one tylko siłą.
        #Tu są wymienione według siły:
        #[1] chłop,
        #[2] włócznik,
        #[3] baron,
        #[4] rycerz.
        #
        #Jednostki dają pobliskim
        #polom obronę w wartości
        #ich siły.
        #
        #By przejąć pole siła
        #jednostek musi być
        #większa niż obrona.
        #
        #Każda jednostka żąda
        #2, 6, 18 lub 54 monet
        #na 1 turę.
        "/>

    <string key="help_trees_article" value="Drzewa pojawiają się po
        #martwych jednostkach.
        #
        #Pole z drzewem nie daje
        #zysku. Znaczy to, że
        #drzewa ranią ekonomie.
        #
        #Musisz się ich pozbyć,
        #ponieważ zaczną się
        #rozprzestrzeniać oraz
        #zabiją całą ekonomie.
        #
        #Palmy pojawiają się przy
        #krańcach i rosną bardzo
        #szybko. Muisz się ich prędko
        #pozbyć.
        "/>

    <string key="help_towers_article" value="Wieże służą do obrony
        #pobliskich pól.
        #
        #Wieże dają obronę [2],
        #więc tylko baronowie i
        #rycerze mogą je pokonać.
        #
        #Chaty dają obronę [1].
        #
        #Na początku gry wieże są
        #najlepszym sposóbem by
        #bronić swojego terytorium.
        #Są one tanie oraz
        #nie żądają pieniędzy.
        "/>

    <string key="help_money_article" value="Zysk zależy od
        #liczby pól.
        #Większe terytorium
        #to większy zysk.
        #Drzewa zmniejszają zysk.
        #
        #Jednostki żądają pieniądzy.
        #Silniejsze jednostki
        #żądają ich więcej.
        #
        #
        #Gdy skończą się pieniądze,
        #jednostki zginą.
        #
        #Zysk jest pokazany
        #w górnej części ekranu
        #na środku.
        "/>

    <string key="help_tactics_article" value="Na początku gry
        #przejmij neutralne pola.
        #Po tym broń wszystkich
        #swoich pól.
        #
        #Upewnij się że zysk
        #jest wystarczający
        #by wynająć jednostki.
        #
        #
        #
        #Dziel wrogie terytoria
        #by zniszczyć ich ekonomię.
        "/>

    <string key="about_complicated_mode_article" value="Włączenie trybu
        #skomplikowanego dodaje więcej menu.
        #W odróżnieniu od prostego
        #dostępne jest:
